<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="C971_Reserve.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:C971_Reserve"
    Shell.FlyoutBehavior="Disabled"
    Title="C971_Reserve"
    NavBarIsVisible="False">

    <ShellContent
        ContentTemplate="{DataTemplate local:TermsManagement}"
        Route="TermsManagement" />
    <ShellContent
        ContentTemplate="{DataTemplate local:EditTerm}"
        Route="EditTerm" />
    <ShellContent
        ContentTemplate="{DataTemplate local:AddTerm}"
        Route="AddTerm" />

</Shell>
